{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "QMTRYReadOnly",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::<ACCOUNT_ID>:role/<QMTRY_ROLE_NAME>"
      },
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::qmtry-demo-synthea",
        "arn:aws:s3:::qmtry-demo-synthea/*"
      ],
      "Condition": {
        "Bool": {
          "aws:SecureTransport": "true"
        }
      }
    }
  ]
}